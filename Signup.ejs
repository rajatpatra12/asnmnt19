<% include ./header %>
<% include ./navbar %>
<div class="container">
    <form method="POST" action="/signup-details">
        <div class="form-group">
            <label for="email">Email:</label>
            <input type="email" class="form-control" id="email" placeholder="Enter email" name="email">
        </div>
        <div class="form-group">
            <label for="upwd" class="control-label">Password:</label>
            <input type="password" id="upwd" name="upwd" class="form-control" placeholder="Enter password">
        </div>
        <div class="form-group">
            <label for="uname" class="control-label">Username:</label>
            <input type="text" id="uname" name="uname" class="form-control" placeholder="Enter username">
       </div>
       
       <button type="submit" class="btn btn-default">Submit</button>
    </form>
</div>
<% include ./footer %>